@misc{blas_protecting_2018,
  title = {Presentation: {{Protecting}} a Cultural Icon and Food Resource: {{Current}} Research and Status of {{Coconut}} Palm in {{Guam}} and the {{Northern Marianas}}. {{Joint Meeting}} of the {{American Phytopathological Society}} ({{APS}}), {{Pafici Division}} and {{Conference}} on {{Soilborne Plant Pathogens}} ({{CSPP}}), {{Portland}}, {{Oregon}}. 2018-06-27.},
  author = {Blas, Andrea L. and Quitugua, Roland and Moore, Aubrey},
  date = {2018-06-27},
  url = {https://www.apsnet.org/members/community/divisions/pac/meetings/Documents/APS_PacificDivisionCSPP_2018_PROGRAM%20SCHEDULE.pdf},
  urldate = {2018-08-25},
  abstract = {Coconut palm is both an important food resource and a cultural icon for Guam and all tropical islands of the Pacific.  Guam in particular among the Insular Pacific islands has been especially hard hit by invasive species due to its centuries-long history as a transportation hub and strategic military location.  Health of the overall coconut palm population in Guam is declining more rapidly in recent years since Coconut rhinoceros beetle (CRB) was first detected in 2007 in Tumon.  Tinangaja, a slow-acting, lethal disease caused by the Coconut tinangaja viroid, was previously known only in Guam.  Prior to the introduction of CRB, Tinangaja disease was reported to affect 30\% of the coconut palm population.  Anecdotal reports of the disease in Rota and Tinian have emerged and visual diagnosis confirmed by the authors.  Results of currently on-going surveys for Tinangaja in Guam and the Northern Mariana Islands will be presented.  Additionally, the potential impact of CRB expansion on Tinangaja disease incidence will be presented with an overview of CRB management and biocontrol programs in Guam.},
  keywords = {Moore-presentations-after-2017,presentation2018,rlg}
}


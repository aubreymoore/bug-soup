@article{caiQueryingGeotaggedVideos2017,
  title = {Querying Geo-Tagged Videos for Vision Applications Using Spatial Metadata},
  author = {Cai, Yinghao and Lu, Ying and Kim, Seon Ho and Nocera, Luciano and Shahabi, Cyrus},
  date = {2017-02-23},
  journaltitle = {EURASIP Journal on Image and Video Processing},
  shortjournal = {EURASIP Journal on Image and Video Processing},

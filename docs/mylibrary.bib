@article{workArrivalRateNonindigenous2005,
  title = {Arrival Rate of Nonindigenous Insect Species into the {{United States}} through Foreign Trade},
  author = {Work, Timothy T. and McCullough, Deborah G. and Cavey, Joseph F. and Komsa, Ronald},
  date = {2005-03-01},

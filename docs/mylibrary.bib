@article{toral-granda_alien_2017,
  title = {Alien Species Pathways to the {{Galapagos Islands}}, {{Ecuador}}},
  author = {Toral-Granda, M. Verónica and Causton, Charlotte E. and Jäger, Heinke and Trueman, Mandy and Izurieta, Juan Carlos and Araujo, Eddy and Cruz, Marilyn and Zander, Kerstin K. and Izurieta, Arturo and Garnett, Stephen T.},
  date = {2017-09-13},
  journaltitle = {PLoS ONE},

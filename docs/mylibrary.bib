@article{uphamLiberatingHostvirusKnowledge2021,
  title = {Liberating Host-Virus Knowledge from {{COVID-19}} Lockdown},
  author = {Upham, Nathan and Poelen, Jorrit H. and Paul, Deborah Leo and Groom, Quentin and Simmons, Nancy B. and Vanhove, Maarten P. M. and Bertolino, Sandro and Reeder, DeeAnn M. and Bastos-Silveira, Cristiane and Sen, Atriya and Sterner, Beckett and Franz, Nico and Guidoti, Marcus and Penev, Lyubomir and Agosti, Donat},
  date = {2021-01-15T21:15:02},
  publisher = {{EcoEvoRxiv}},
  doi = {10.32942/osf.io/txekq},
  url = {https://ecoevorxiv.org/txekq/},
  urldate = {2021-01-17},
  abstract = {Connecting basic data about bats and other potential mammal hosts of SARS-CoV-2 with their ecological context is now critical for understanding the emergence and spread of COVID-19. However, when global lockdown started in March 2020, the world’s bat experts were locked out of their research laboratories, which, in turn, locked up large volumes of offline ecological and taxonomic data. Global lockdown has put a magnifying glass on the long-standing problem of biological ‘dark data’ that is published but disconnected from digital knowledge resources, and thus unavailable for high-throughput analysis. Host-to-virus knowledge will be biased until this challenge is addressed. Here we outline two viable solutions: (i) how to interconnect published data about mammal hosts and viruses in the short term; and (ii) how to shift the publishing paradigm beyond unstructured text (e.g., PDFs) to labeled networks of digital knowledge. Biological taxonomy is foundational to both solutions as the indexing system for biodiversity data. Building digitally connected ‘knowledge graphs’ of host-virus interactions will establish the needed agility for quickly identifying reservoir hosts of novel zoonoses, allow for AI-based predictions of emergence, and thereby improve planetary health security.},
  keywords = {artificial intelligence,Diseases,Epidemiology,FAIR,International Public Health,mammal,Medicine and Health Sciences,open-access,Organisms,Public Health,spillover,virus,zoonosis},
  file = {/home/aubrey/Zotero/storage/P2HR4QG8/Upham et al. - 2021 - Liberating host-virus knowledge from COVID-19 lock.pdf}
}

